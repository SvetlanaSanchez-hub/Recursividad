        "lista = [ 2, 4, 6, 8]\n",
        "def sumaRecursiva ( lista ):\n",
        "    if len( lista ) == 0:\n",
        "        return 0\n",
        "    else:\n",
        "        return lista [ 0 ] + sumaRecursiva( lista [1:] )\n",
        "print(sumaRecursiva(lista))\n",
        "                                "
